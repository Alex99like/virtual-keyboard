{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base/legacy",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-shadow": "off",
        "no-restricted-syntax": "off",
        "object-curly-newline": "off",
        "class-methods-use-this": "off",
        "no-param-reassign": "off",
        "no-case-declarations": "off",
        "max-len":"off"
    }
}